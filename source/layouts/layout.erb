<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "Jane Macfarlane" %></title>
    
    <%= stylesheet_link_tag "reset", "main" %>
    <%= javascript_include_tag  "all" %>
  </head>
  <body class="<%= page_classes %>">
  <div id="page">
    <div id="header">
      <img src="/images/doctorjanebannergraphic.jpg">
    </div>

    <div id="nav">
      <ul>
        <li class="selected">
          <a href="/" id="indexnav">Home</a>
        </li>
        <li>
          <a href="/business-process" id="business-processnav">Business Process</a>
        </li>
        <li>
          <a href="/industry-process-maps" id="industry-process-mapsnav">Industry Process Maps</a>
        </li>
        <li>
          <a href="/software-and-technology" id="software-and-technologynav">Software and Technology</a>
        </li>
        <li>
          <a href="/technology-management" id="technology-managementnav">Technology Management</a>
        </li>
      </ul>
    </div>
    <div id="main">
      <div id="content-wrapper">
        <div id="content">
          <%= yield %>
        </div>
      </div>
      <div id="sidebar-wrapper">
        <div id="sidebar">
          <p>
            <img src="/images/tisrlogo.jpg">
          </p>
          <%= yield_content :sidebar %>
        </div>
      </div>
    </div>
    <div style="clear:both">
    </div>
  </div>

  <div id="footer-wrapper">
    <div id="footer">
      Copyright Â© 2013 Thinking Is Still Required. All rights reserved |
      <a href="mailto:info@doctorjane.org">Contact</a>
    </div>
  </div>
  </body>
</html>